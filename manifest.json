{
	"manifest_version": 2,
	"name": "Temporary Mail",
	"description": "Temporary Mail",
	"version": "1.0",
	"icons": {
		 "16": "images/logo.16.png",
		 "48": "images/logo.48.png",
		"128": "images/logo.128.png"
	},
	"background": {
		"scripts": ["js/app.js","js/bg.js"]
	},
	"content_scripts": [{
		"js": ["js/cs.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_start",
		"all_frames": true
	}],
	"browser_action": {
          "default_title": "Temporary Mail",
          "default_popup": "popup.html"
        },
	"permissions": [
		"<all_urls>",
		"activeTab",
		"tabs",
		"storage",
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"notifications",
		"cookies"
	]
}
